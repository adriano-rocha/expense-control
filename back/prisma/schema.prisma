generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Transaction {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  description String
  amount      Float
  type        String   
  createdAt   DateTime @default(now())
  
  @@map("transactions")
}